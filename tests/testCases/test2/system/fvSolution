FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
  p
    {
      solver     GAMG;
      tolerance 1e-06;
      relTol     0.01;
      smoother  DIC; 
      cacheAgglomeration true;
      nCellsInCoarsestLevel 10;
      agglomerator faceAreaPair;
      mergeLevels   1;
    }
  pFinal
    {
      $p;
      smoother   DICGaussSeidel;
      relTol     0;
      smoother   DIC;
    }

    "(U|k)"
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-05;
        relTol          0.1;
    }

    "(U|k)Final"
    {
        $U;
        reTol 0;
    }

/*    "(U|k)"
